<%- include layouts/header.ejs %>

        <div class="page-data">
            
            <form method="post" action="/encuesta/edit/<%=data[0].id%>">
                <table cellpadding="11">
                <tr>
                    <td class="label">Administrador Id</td><td>: <input type="text" name="adminitrador_id" value="<%=data[0].adminitrador_id%>" required></td>
                 </tr>
                 <tr>
                    <td class="label" valign="top">Titulo</td>
                    <td valign="top">:
                        <textarea name="titulo" required cols="30" rows="5"><%=data[0].titulo%></textarea>
                    </td>
                </tr>
                <tr>
                    <td class="label">Fecha Inicio</td><td>: <input type="text" value="<%=data[0].fechainicio%>" name="fechainicio" required></td>
                 </tr>
                  <tr>
                    <td class="label">Fecha Fin</td><td>: <input type="text" name="fechafin" value="<%=data[0].fechafin%>" required></td>
                 </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Guardar">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>
